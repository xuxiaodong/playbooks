[log]
	path = {{ path }}
	use chroot = no
	max connections=10
	lock file = /var/lock/rsyncd
	read only = yes
	list = no
	uid = nobody
	gid = nobody
#	auth users = 
#	secrets file = /etc/rsyncd.secrets
	strict modes = yes
	hosts allow = {{ allow_host }}
    hosts deny = *
	transfer logging = no
	timeout = 600
